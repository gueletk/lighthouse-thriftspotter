
<div class="row">
<div class="col-md-12"><h1>Map Search</h1></div>

<div class="col-md-12">
<%@items = Item.all%>
<%@items.each_with_index do |item, index|%>
  <div data-id="<%=item.id%>" data-index="<%= index %>" data-title="<%=item.title%>" data-placeid="<%=item.place_id%>" data-long="<%=item.longitude%>" data-lat="<%=item.latitude%>" class="item">
   
  </div>
<%end%>
  <div style="height:800px" id="map">

  </div>

  <script src="/javascript/map_marker.js"></script>
  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCTbeQc-SCnCKLpYePwXmYPwXcvd_LRGtk&libraries=places&callback=initMap"></script>

</div>

</div>


